<%= form_with(model: @prototype, local: true) do |f|%>
  <div class="field">
    <%= f.label :name, "スポットの名称" %><br />
    <%= f.text_field :name %>
  </div>

<div class="field">
    <%= f.label :genre, "ジャンル" %><br />
    <%= f.text_field :genre, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :place_id, "場所" %><br />
    <%= f.text_field :place_id, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :explanation, "説明" %><br />
    <%= f.text_area :explanation, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :plan, "プラン" %><br />
    <%= f.text_area :plan, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :wifi_id, "Wifi(通信環境)" %><br />
    <%= f.text_field :wifi_id, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :budget, "予算" %><br />
    <%= f.text_area :budget, class: :form__text %>
  </div>

  <div class="field">
    <%= f.label :image, "スポットの画像" %><br />
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "保存する", class: :form__btn  %>
  </div>
<% end %>